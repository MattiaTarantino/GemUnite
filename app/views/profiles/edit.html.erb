<!-- app/views/profiles/edit.html.erb -->
<h1>Edit Profile</h1>
<%= form_for @user, url: profile_path, method: :patch, html: { multipart: true } do |f| %>


  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>


  <%= f.submit "Save Changes" %>
<% end %>